{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "plugins": ["jest"],
  "extends": ["airbnb-base", "plugin:jest/recommended"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "comma-dangle": [
      "error",
      {
        "objects": "only-multiline"
      }
    ]
  }
}
